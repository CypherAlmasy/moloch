  "scripts": {
    "start": "node viewer.js -c ../config.ini",
    "start:test": "node viewer.js --debug -c ../tests/config.test.ini -n test & webpack -d --watch --display-error-details",
    "start:testuser": "node viewer.js -c ../tests/config.test.ini -n testuser & webpack -d --watch --display-error-details",
    "start:testuserdebug": "node viewer.js -c ../tests/config.test.ini -n testuser --debug & webpack -d --watch --display-error-details",
    "addtestuser": "node addUser.js -c ../tests/config.test.ini -n testuser admin admin admin --admin",
    "start:dev": "node viewer.js -c config.dev.ini & webpack -d --watch --display-error-details",
    "prestart": "npm run bundle:min",
    "build": "npm run test && npm run bundle:min",
    "lint": "jshint app components",
    "viewerlint": "jshint viewer.js db.js config.js multies.js",
    "bundle": "webpack --progress --display-error-details --bail",
    "bundle:min": "webpack --define process.env.NODE_ENV=production --progress --bail",
    "test": "karma start --single-run --debug",
    "test:watch": "karma start --auto-watch",
    "pretest": "npm run lint",
    "pretest:watch": "npm run lint",
    "bundle:vue:min": "node vueapp/build/build.js",
    "bundle:vue:dev": "webpack --progress --hide-modules --config vueapp/build/webpack.dev.conf.js",
    "start:vue:dev": "node viewer.js -c ../tests/config.test.ini -n testuser --debug & webpack --progress --hide-modules -d --watch --config vueapp/build/webpack.dev.conf.js",
    "start:vue:dev2": "node viewer.js -c config.dev.ini & webpack --progress --hide-modules -d --watch --config vueapp/build/webpack.dev.conf.js",
    "prestart:vue:dev": "webpack -d --display-error-details"
  },
